<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/KAPAK"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#7EEA7C"
    android:foreground="@drawable/dolar_png"
    tools:context=".MainActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="0dp">

    <EditText
        android:id="@+id/enkazancli_kapak"
        style="@android:style/Widget.Holo.Light.AutoCompleteTextView"
        android:layout_width="364dp"
        android:layout_height="185dp"
        android:ems="10"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="                         ENKAZANÇLI"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="317dp" />

    <EditText
        android:id="@+id/designed_atahan"
        android:layout_width="309dp"
        android:layout_height="43dp"
        android:ems="10"
        android:fontFamily="sans-serif-condensed"
        android:inputType="textPersonName"
        android:text="     Designed By Atahan Karakamışlıoğlu"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="689dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="77dp"
        android:layout_height="82dp"
        android:rotationX="0"
        app:srcCompat="@drawable/tl_png"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="250dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="77dp"
        android:layout_height="82dp"
        android:rotationX="0"
        app:srcCompat="@drawable/euro_png"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="250dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="77dp"
        android:layout_height="82dp"
        android:rotationX="0"
        app:srcCompat="@drawable/dolar_png"
        tools:layout_editor_absoluteX="250dp"
        tools:layout_editor_absoluteY="250dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
